@* Отличие ViewComponent от PartalView  в том, что первый имеет контроллер *@
@model IEnumerable<CategoriesHeadCount>

<table class="table table-hover rounded-pill">
    <thead class="thead-light">
        <tr>
            <th>Категории</th>
            <th>Товаров: </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Categories</td>
                <td>@item.Count</td>
            </tr>
        }
    </tbody>
</table>
